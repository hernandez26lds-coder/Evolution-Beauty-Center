
{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**/index.tsx",
        "destination": "/index.tsx"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
